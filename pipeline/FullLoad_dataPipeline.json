							"outputs": [
								{
									"referenceName": "DS_SQL_Abhishek",
									"type": "DatasetReference",
									"parameters": {
										"schmeaname": {
											"value": "@item().schmeaname",
											"type": "Expression"
										},
										"tablename": {
											"value": "@item().tablename",
											"type": "Expression"
										}
