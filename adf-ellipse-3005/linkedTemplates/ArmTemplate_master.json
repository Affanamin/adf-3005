{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-ellipse-3005"
		},
		"LinkedServiceForSourceToCurated_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LinkedServiceForSourceToCurated'"
		},
		"LS_KeyVaultForSqlSecrets_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvault-EllipseProject.vault.azure.net/"
		},
		"LS_SqlDatabaseEllipse_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ellipse-sql-server.database.windows.net"
		},
		"LS_SqlDatabaseEllipse_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "nayapaysql"
		},
		"LS_SqlDatabaseEllipse_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "cloudsql"
		},
		"LinkedServiceForSourceToCurated_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saellipse3005gen2.dfs.core.windows.net/"
		},
		"LS_SqlServerOnPrem_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-QR891Q3\\SQLEXPRESS"
		},
		"LS_SqlServerOnPrem_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorksLT2017"
		},
		"LS_SqlServerOnPrem_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mrk"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-ellipse-3005_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LinkedServiceForSourceToCurated_accountKey": {
						"value": "[parameters('LinkedServiceForSourceToCurated_accountKey')]"
					},
					"LS_KeyVaultForSqlSecrets_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KeyVaultForSqlSecrets_properties_typeProperties_baseUrl')]"
					},
					"LinkedServiceForSourceToCurated_properties_typeProperties_url": {
						"value": "[parameters('LinkedServiceForSourceToCurated_properties_typeProperties_url')]"
					},
					"LS_SqlDatabaseEllipse_properties_typeProperties_server": {
						"value": "[parameters('LS_SqlDatabaseEllipse_properties_typeProperties_server')]"
					},
					"LS_SqlDatabaseEllipse_properties_typeProperties_database": {
						"value": "[parameters('LS_SqlDatabaseEllipse_properties_typeProperties_database')]"
					},
					"LS_SqlDatabaseEllipse_properties_typeProperties_userName": {
						"value": "[parameters('LS_SqlDatabaseEllipse_properties_typeProperties_userName')]"
					},
					"LS_SqlServerOnPrem_properties_typeProperties_server": {
						"value": "[parameters('LS_SqlServerOnPrem_properties_typeProperties_server')]"
					},
					"LS_SqlServerOnPrem_properties_typeProperties_database": {
						"value": "[parameters('LS_SqlServerOnPrem_properties_typeProperties_database')]"
					},
					"LS_SqlServerOnPrem_properties_typeProperties_userName": {
						"value": "[parameters('LS_SqlServerOnPrem_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-ellipse-3005_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-ellipse-3005_ArmTemplate_0')]"
			]
		}
	]
}